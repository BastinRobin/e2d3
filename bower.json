{
  "name": "e2d3",
  "version": "0.4.0",
  "authors": [
    "E2D3"
  ],
  "description": "E2D3 is JavaScript library for using D3.js on Excel.",
  "license": "E2D3",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "requirejs": "~2.1.15",
    "requirejs-domready": "~2.0.1",
    "requirejs-text": "~2.0.13",
    "bootstrap-sass-official": "~3.3.0",
    "d3": "~3.5.5",
    "font-awesome": "~4.4.0",
    "FileSaver": "*",
    "canvg-gabelerner": "*",
    "marked": "~0.3.5",
    "vue": "~1.0.1",
    "colorbrewer": "~1.0.0",
    "js-yaml": "~3.4.3"
  },
  "overrides": {
    "requirejs-domready": {
      "main": "domReady.js"
    },
    "requirejs-text": {
      "main": "text.js"
    },
    "d3.promise": {
      "main": "dist/d3.promise.js"
    },
    "FileSaver": {
      "main": "FileSaver.js"
    },
    "font-awesome": {
      "main": "fonts/*"
    },
    "canvg-gabelerner": {
      "main": [
        "canvg.js",
        "stackblur.js",
        "rgbcolor.js"
      ]
    }
  },
  "resolutions": {
    "d3": "~3.5.5"
  }
}
