{
  "name": "e2d3",
  "version": "0.4.0",
  "authors": [
    "E2D3"
  ],
  "description": "E2D3 is JavaScript library for using D3.js on Excel.",
  "license": "E2D3",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "requirejs": "~2.1.15",
    "requirejs-domready": "~2.0.1",
    "requirejs-text": "~2.0.13",
    "bootstrap-sass-official": "~3.3.0",
    "d3": "~3.5.5",
    "markdown": "~0.5.0",
    "font-awesome": "~4.3.0",
    "jquery-deparam": "~0.3.0",
    "FileSaver": "*",
    "canvg-gabelerner": "*",
    "requirejs-plugins": "~1.0.3"
  },
  "overrides": {
    "requirejs-domready": {
      "main": "domReady.js"
    },
    "requirejs-text": {
      "main": "text.js"
    },
    "markdown": {
      "main": "lib/markdown.js"
    },
    "FileSaver": {
      "main": "FileSaver.js"
    },
    "font-awesome": {
      "main": "fonts/*"
    }
  }
}
